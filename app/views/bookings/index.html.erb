<h1>Bookings:</h1>

<% @bookings.each do |booking| %>
<p><%= link_to booking.description, booking_path(booking) %> <a><%= link_to "Edit", edit_booking_path(booking), :class => 'btn btn-success' %></a>
<%= link_to "Delete",
  booking_path(booking),
  data: {turbo_method: :delete, turbo_confirm: "Are you sure?"} %>
</p>
<% end %>
