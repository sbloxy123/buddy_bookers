<div class="container">
<h1>Sell your friend or sell yourself:</h1>

<%= simple_form_for(@friend) do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :location %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :photo, as: :file %>
  <%= f.submit %>
<% end %>
</div>
